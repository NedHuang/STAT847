ms <- seq(from=5, to=15)
get_ratio <- function(p,n,ms){
ans = c()
for (i in 1:length(ms)){
ans <- c(ans, p^(n-ms[i]))
}
return(ans)
}
ratios <- get_ratio(1/2,5,ms)
print(ratios)
plot(ms,ratios,xlab="value of m",ylab="ratio",col="blue")
